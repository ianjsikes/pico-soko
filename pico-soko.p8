-- update function --
---------------------

function _update()
  if freeze>0 then freeze-=1 return end

  if shake>0 then
    shake-=1
    camera()
    if shake>0 then
      camera(-2+rnd(5),-2+rnd(5))
    end
  end

  -- if will_restart and delay_restart>0 then
  --   delay_restart-=1
  --   if delay_restart<=0 then
  --     will_restart=false
  --     load_room(room.x,room.y)
  --   end
  -- end
  if btnp(d.restart) then
    -- load_room(0,0)
    sfx(s.restart)
    load_room(room.x,room.y)
    -- return
  end

  if btnp(d.skip) then next_room() end

  foreach(objects,function(obj)
    if obj.type.update then
      obj.type.update(obj)
    end
  end)

  local num_goals = count(goals)
  local num_filled = 0
  foreach(goals,function(goal)
    if goal.filled then
      num_filled+=1
    end
  end)

  if num_goals>0 and num_goals==num_filled then
    if level_index()>=num_rooms then
      -- game over
      sfx(s.win_game)
      load_room(0,0)
    else
      next_room()
    end
  end
end

-- drawing functions --
-----------------------

function _draw()
  if freeze>0 then return end

  cls()
  map(room.x*16,room.y*16,0,0,16,16,1)
  map(room.x*16,room.y*16,0,0,16,16,2)
  print('level ' .. level_index(),3,3,10)
  print('restart: z',3,120,10)

  foreach(objects,draw_object)

  -- draw outside of the screen for screenshake
	rectfill(-5,-5,-1,133,0)
	rectfill(-5,-5,133,-1,0)
	rectfill(-5,128,133,133,0)
	rectfill(128,-5,133,133,0)
end

function draw_object(obj)
  if obj.type.draw then
    obj.type.draw(obj)
  elseif obj.spr > 0 then
    spr(obj.spr,obj.x,obj.y,1,1,obj.flip)
  end
end

-- helper functions --
----------------------

function tile_at(x,y)
  return mget(room.x*16+x,room.y*16+y)
end

function tile_flag_at(x,y,flag)
  return fget(tile_at(x,y))==flag
end

--[[
function moveplayer(x,y)
 local m=mget((x+player.x)/8,(y+player.y)/8)
 local move=fget(m)!=1
 if m==ball then
  local b=mget((2*x+player.x)/8,(2*y+player.y)/8)
  if fget(b)!=1 then
   sfx(0)
   mset((2*x+player.x)/8,(2*y+player.y)/8,ball)
   mset((x+player.x)/8,(y+player.y)/8,0)
  else
   move=false
  end
  if b==goal then
   sfx(1)
   gameover=true
  end
 end
 if move then
  player.x+=x
  player.y+=y
  --cycle through walk anim
  player.sp-=1
  if player.sp==0 then
   player.sp=3
  end
  --flip the player sprite
  --when moving left
  player.fl=x<0
 end
end

function _update()
 if gameover then t+=1
 else
  if btnp(0) then moveplayer(-8, 0) end
  if btnp(1) then moveplayer( 8, 0) end
  if btnp(2) then moveplayer( 0,-8) end
  if btnp(3) then moveplayer( 0, 8) end
 end
 if t>60 then load('soko.p8') end
end

function _draw()
 cls()
 map(0,0,0,0,16,16)
 spr(player.sp,player.x,player.y,1,1,player.fl)
end

]]--

