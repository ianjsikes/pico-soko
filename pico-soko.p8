version 8
__lua__
-- �pico-soko�
-- ian j sikes

-- globals --
-------------

room={x=0,y=0}
num_rooms=8
goals={}
objects={}
types={}
freeze=0
shake=0
pause_player=false

d={left=0,right=1,up=2,down=3,restart=4,skip=5}
f={col=1}
t={floor=0,player=1,ball=17,goal=18,ball_goal=19}
s={bump=0,win_level=1,restart=2,win_game=3}

-- entry point --
-----------------

function _init()
  load_room(0,0)
end

function level_index()
  return (room.x%8+room.y*8) + 1
end

function load_room(x,y)
  foreach(objects,destroy_object)
  goals={}

  room.x=x
  room.y=y

  for tx=0,15 do
    for ty=0,15 do
      local tile=mget(room.x*16+tx,room.y*16+ty)
      if tile==t.ball_goal then
        init_object(ball,tx*8,ty*8)
      else
        foreach(types, function(type)
          if type.tile==tile then
            init_object(type,tx*8,ty*8)
            -- mset(room.x*16+tx,room.y*16+ty,t.floor)
          end
        end)
      end
    end
  end
end

function next_room()
  sfx(s.win_level)
  if room.x==7 then
    load_room(0,room.y+1)
  else
    load_room(room.x+1,room.y)
  end
end

-- object functions --
----------------------

function init_object(type,x,y)
  local obj={}
  obj.type=type
  obj.collideable=true
  obj.spr=type.tile
  obj.flip=false
  obj.x=x
  obj.y=y
  obj.hitbox={x=0,y=0,w=8,h=8}

  obj.collide=function(type,ox,oy)
    local other
    for i=1,count(objects) do
      other=objects[i]
      if other~=nil and other.type==type and other!=obj and other.collideable and
        other.x+other.hitbox.x+other.hitbox.w > obj.x+obj.hitbox.x+ox and
        other.y+other.hitbox.y+other.hitbox.h > obj.y+obj.hitbox.y+oy and
        other.x+other.hitbox.x < obj.x+obj.hitbox.x+obj.hitbox.w+ox and
        other.y+other.hitbox.y < obj.y+obj.hitbox.y+obj.hitbox.h+oy then
        return other
      end
    end
    return nil
  end

  obj.check=function(type,ox,oy)
    return obj.collide(type,ox,oy)~=nil
  end

  if obj.type==ball then
    add(goals,obj)
  end

  add(objects,obj)
  if obj.type.init then
    obj.type.init(obj)
  end
  return obj
end

function destroy_object(obj)
  del(objects,obj)
end

-- entities --
--------------

player =
{
	tile=t.player,
	init=function(this)

	end,
	update=function(this)
	  if pause_player then return end

    local dx,dy,fl = this.type.process_input(this)
    this.flip=fl
    if dx!=0 or dy!=0 then
      this.spr-=1
      if this.spr==0 then this.spr=3 end

      if not tile_flag_at(this.x/8+dx,this.y/8+dy,f.col) then
        local hit=this.collide(ball,dx,dy)
        if hit~=nil then
          -- move ball
          if not tile_flag_at(hit.x/8+dx,hit.y/8+dy,f.col) then
            local ball_hit=hit.collide(ball,dx,dy)
            if ball_hit==nil then
              hit.x+=dx*8
              hit.y+=dy*8
              this.x+=dx*8
              this.y+=dy*8
              sfx(s.bump)
            end
          end
        else
          this.x+=dx*8
          this.y+=dy*8
        end
      end
    end
	end,
	draw=function(this)
		spr(this.spr,this.x,this.y,1,1,this.flip)
	end,
  process_input=function(this)
    local dx=0
    local dy=0
    local fl=this.flip
    if btnp(d.left) then
      dx-=1
      fl=true
    end
    if btnp(d.right) then
      dx+=1
      fl=false
    end
    if btnp(d.up) then dy-=1 end
    if btnp(d.down) then dy+=1 end
    return dx,dy,fl
  end
}
add(types,player)

ball =
{
	tile=t.ball,
  update=function(this)
    local ti = tile_at(this.x/8,this.y/8)
    if ti==t.goal or ti==t.ball_goal then
      this.filled=true
      this.spr=16
    else
      this.filled=false
      this.spr=17
    end
  end
}
add(types,ball)

